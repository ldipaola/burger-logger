<h1>Eat-Da-Burgers!</h1>



<ul>
  {{#each burgers}}
  {{#unless eaten }}
  <li>{{ burger }}</li>
  {{/unless}}
  {{/each}}
</ul>
<br>
<ul>
  {{#each burgers}}
  {{#if eaten }}
  <li>{{ burger }}</li>
  {{/if}}
  {{/each}}
</ul>

<form class="create-form">

  <div class="form-group">
    <label for="burg">Add a burger:</label>
    <input type="text" id="burg" name="burger">
  </div>

  <button type="submit">Add Burger!</button>
</form>